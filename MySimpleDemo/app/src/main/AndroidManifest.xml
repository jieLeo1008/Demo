<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jieleo.mysimpledemo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".java.activity.WelcomeActivity"
            android:theme="@style/AppTheme.Base">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".kotlin.MainActivity">
        </activity>
        <activity android:name=".java.activity.ExtractQRCodeActivity">
        </activity>
        <activity android:name=".java.activity.ViewFilperAdActivity"/>
        <activity android:name=".java.activity.ZhiHuMatisseActivity"/>
        <activity android:name=".java.activity.CollapsingToolbarLayoutActivity"/>
        <activity android:name=".java.activity.CountDownActivity">
        </activity>
        <activity android:name=".java.activity.CountDownTimeActivity">
        </activity>
    </application>

</manifest>