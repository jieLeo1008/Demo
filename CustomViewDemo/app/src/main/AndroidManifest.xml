        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.PassWordActivity">
        </activity>
        <activity android:name=".activity.ItemDecorationActivity">
        </activity>
        <activity android:name=".activity.ObjectAnimatorActivity">
        </activity>
        <activity android:name=".activity.BGABadgeViewActivity">
        </activity>
        <activity android:name=".activity.TextInputLayoutActivity">
        </activity>
        <activity android:name=".activity.CaiPiaoActivity">
        </activity>
        <activity android:name=".activity.CatulatorActivity">
        </activity>
        <activity android:name=".activity.QRCodeActivity">
        </activity>
        <activity android:name=".activity.DLTActivity">
        </activity>
        <activity android:name=".activity.CatulatorBitmapActivity">
        </activity>
        <activity android:name=".activity.MyItemActivity">
        </activity>
        <activity android:name=".activity.ImmersiveActivity">
        </activity>
    </application>

</manifest>